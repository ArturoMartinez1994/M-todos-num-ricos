        "import sympy as sp\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "x0=1\n",
        "n=4\n",
        "x=sp.Symbol('x')\n",
        "t=sp.log(x).series(x,x0,n).removeO()\n",
        "print(t)\n",
        "t\n",
        "t=sp.lambdify(x,t,'numpy')\n",
        "xx=np.linspace(x0-5,x0+5,1000)\n",
        "yc=np.log(xx)\n",
        "yt=t(xx)\n",
        "plt.plot(xx,yc,xx,yt)\n",
        "plt.legend(['log(x)','Serie de Taylor orden 4'])\n",
        "plt.title('Serie de Taylor de $f(x)=log(x)$')\n",
        "plt.grid(True)\n"
      ]
